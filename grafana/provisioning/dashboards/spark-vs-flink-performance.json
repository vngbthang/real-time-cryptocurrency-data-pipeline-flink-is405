        {
          "datasource": {
            "type": "postgres",
            "uid": "postgres"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \n  'Spark' as metric,\n  AVG(EXTRACT(EPOCH FROM (processed_at - TO_TIMESTAMP(timestamp)))) as value\nFROM crypto_prices_realtime\nWHERE processed_at > NOW() - INTERVAL '5 minutes'\nUNION ALL\nSELECT \n  'Flink' as metric,\n  AVG(EXTRACT(EPOCH FROM (processed_at - TO_TIMESTAMP(timestamp)))) as value\nFROM crypto_prices_flink\nWHERE processed_at > NOW() - INTERVAL '5 minutes'",
          "refId": "A",

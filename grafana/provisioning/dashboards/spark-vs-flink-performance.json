      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "postgres"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH spark_latencies AS (\n  SELECT EXTRACT(EPOCH FROM (processed_at - TO_TIMESTAMP(timestamp))) as latency\n  FROM crypto_prices_realtime\n  WHERE processed_at > NOW() - INTERVAL '5 minutes'\n),\nflink_latencies AS (\n  SELECT EXTRACT(EPOCH FROM (processed_at - TO_TIMESTAMP(timestamp))) as latency\n  FROM crypto_prices_flink\n  WHERE processed_at > NOW() - INTERVAL '5 minutes'\n)\nSELECT \n  'Spark p50' as metric,\n  PERCENTILE_CONT(0.50) WITHIN GROUP (ORDER BY latency) as value\nFROM spark_latencies\nUNION ALL\nSELECT \n  'Flink p50' as metric,\n  PERCENTILE_CONT(0.50) WITHIN GROUP (ORDER BY latency) as value\nFROM flink_latencies\nUNION ALL\nSELECT \n  'Spark p95' as metric,\n  PERCENTILE_CONT(0.95) WITHIN GROUP (ORDER BY latency) as value\nFROM spark_latencies\nUNION ALL\nSELECT \n  'Flink p95' as metric,\n  PERCENTILE_CONT(0.95) WITHIN GROUP (ORDER BY latency) as value\nFROM flink_latencies\nUNION ALL\nSELECT \n  'Spark p99' as metric,\n  PERCENTILE_CONT(0.99) WITHIN GROUP (ORDER BY latency) as value\nFROM spark_latencies\nUNION ALL\nSELECT \n  'Flink p99' as metric,\n  PERCENTILE_CONT(0.99) WITHIN GROUP (ORDER BY latency) as value\nFROM flink_latencies",
          "refId": "A"
        }
      ],
      "title": "Latency Percentiles (p50, p95, p99)",
      "type": "bargauge"

      "pluginVersion": "10.2.0",
      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "postgres"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \n  'Spark' as metric,\n  COUNT(*) * 60.0 / EXTRACT(EPOCH FROM (MAX(processed_at) - MIN(processed_at))) as value\nFROM crypto_prices_realtime\nWHERE processed_at > NOW() - INTERVAL '5 minutes'\nUNION ALL\nSELECT \n  'Flink' as metric,\n  COUNT(*) * 60.0 / EXTRACT(EPOCH FROM (MAX(processed_at) - MIN(processed_at))) as value\nFROM crypto_prices_flink\nWHERE processed_at > NOW() - INTERVAL '5 minutes'",
          "refId": "A"
        }
      ],
      "title": "Throughput (records/min) - Higher is Better",
      "type": "gauge"
    },
